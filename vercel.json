{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/contractor-reminders",
      "schedule": "0 */1 * * *"
    },
    {
      "path": "/api/won-bid-followups",
      "schedule": "0 */1 * * *"
    }
  ]
}
